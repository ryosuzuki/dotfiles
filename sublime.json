{
  "auto_indent": true,
  "color_scheme": "Packages/Color Scheme - Default/Monokai.tmTheme",
  "draw_indent_guides": true,
  "draw_white_space": "all",
  "file_exclude_patterns":
  [
    "*.pyc",
    "*.pyo",
    "*.exe",
    "*.dll",
    "*.o",
    "*.a",
    "*.lib",
    "*.so",
    "*.dylib",
    "*.ncb",
    "*.sdf",
    "*.suo",
    "*.pdb",
    "*.idb",
    ".DS_Store",
    "*.class",
    "*.psd",
    "*.sublime-workspace"
  ],
  "font_size": 18.0,
  "ignored_packages":
  [
    "AlignTab",
    "All Autocomplete",
    "AutoFileName",
    "Better CoffeeScript",
    "BracketHighlighter",
    "Case Conversion",
    "CoffeeScriptHaml",
    "EJS",
    "Emacs Pro Essentials",
    "GitGutter",
    "Github Color Theme",
    "Jade",
    "Keymaps",
    "LESS",
    "Markdown",
    "MarkdownHighlighting",
    "Move By Paragraph",
    "Numix Theme",
    "OpenGL Shading Language (GLSL)",
    "Origami",
    "PackageResourceViewer",
    "Processing",
    "Python 3",
    "Sass",
    "SublimeServer",
    "Theme - Glacier",
    "Vintage",
    "zenburn"
  ],
  "indent_guide_options":
  [
    "draw_normal",
    "draw_active"
  ],
  "smart_indent": true,
  "tab_size": 2,
  "translate_tabs_to_spaces": true,
  "trim_trailing_white_space_on_save": true,
  "update_check": false,
  "word_wrap": true
}
